- unless current_user
  %li
    %a{ href: '/users/sign_in' }
      Sign In
  %li
    %a{ href: '/users/sign_up' }
      Sign Up
- else

  %li.dropdown
    %a.dropdown-toggle{ href: '#', 'data-toggle': 'dropdown' }
      = current_user.email
      %b.caret
    %ul.dropdown-menu
      %li
        = link_to 'Logout', destroy_user_session_path, method: :delete